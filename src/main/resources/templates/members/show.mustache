{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Email</th>
        <th scope="col">Password</th>
        <th scope="col">Name</th>
        <th scope="col">BirthYear</th>
        <th scope="col">BirthMonth</th>
        <th scope="col">BirthDay</th>
        <th scope="col">Phone</th>
        <th scope="col">Gender</th>
        <th scope="col">Region1</th>
        <th scope="col">Region2</th>
        <th scope="col">Region3</th>


    </tr>
    </thead>
    <tbody>
    {{#member}}
        <tr>
            <th>{{id}}</th>
            <td>{{email}}</td>
            <td>{{password}}</td>
            <td>{{name}}</td>
            <td>{{birthYear}}</td>
            <td>{{birthMonth}}</td>
            <td>{{birthDay}}</td>
            <td>{{phone}}</td>
            <td>{{gender}}</td>
            <td>{{region1}}</td>
            <td>{{region2}}</td>
            <td>{{region3}}</td>
        </tr>
    {{/member}}
    </tbody>
</table>

<a href="/members/{{member.id}}/edit" class="btn btn-primary">수정하기</a>
<a href="/members/{{member.id}}/delete" class="btn btn-danger">삭제하기</a>
<a href="/members">Go to member list</a>

{{>layouts/footer}}
