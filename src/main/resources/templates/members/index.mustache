{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Email</th>
        <th scope="col">Password</th>
        <th scope="col">Name</th>
        <th scope="col">BirthYear</th>
        <th scope="col">BirthMonth</th>
        <th scope="col">BirthDay</th>
        <th scope="col">Phone</th>
        <th scope="col">Gender</th>
        <th scope="col">Region1</th>
        <th scope="col">Region2</th>
        <th scope="col">Region3</th>
    </tr>
    </thead>
    <tbody>
    {{#members}}
        <tr>
            <th>{{id}}</th>
            <td><a href="/members/{{id}}">{{email}}</a></td>
            <td>{{password}}</td>
        </tr>
    {{/members}}
    </tbody>
</table>

<a href="/members/new">회원가입</a>

{{>layouts/footer}}
